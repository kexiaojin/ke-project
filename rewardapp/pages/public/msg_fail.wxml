<!--pages/public/msg_fail.wxml-->
<view class='header'>
    <icon type="warn" size="90" color="#E64340"></icon>
</view>
<view class='content'>
    <view class="fz-lg">{{info.title}}</view>
    <text>{{info.content}}</text>
</view>
<view class="bottom">
    <button hover-class="btn-hover" type='primary' bindtap="finish">
        返回
    </button>
</view>
