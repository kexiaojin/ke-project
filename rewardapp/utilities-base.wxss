.canvas-box{
  position: absolute;
  left: -9999rpx;
  bottom: 0;
}
.sexbox{
  position: absolute;
  right: -6px;
  top: -6px;
  background: #fff;
  border-radius: 50%;
}
.move-box{
  position: fixed;
  top: 0;
  left: 0;
  pointer-events: none;
}
.nav-box{
  position: absolute;
  right: 30rpx;
  bottom: 200rpx;
  pointer-events: auto;
}
.nav-bar{
  text-align: right;
}
.nav-bar text{
  display: inline-block;
  font-size: 36rpx;
  padding: 30rpx;
  border-radius: 50%;
  background: #444;
  color: #fff;
  border: solid 4rpx #fff;
  box-shadow: 0rpx 0rpx 20rpx rgba(0,0,0,.2);
}
.nav-list{
  position: relative;
  background: #444;
  color: #fff;
  height: 0;
  overflow: hidden;
  opacity: 0;
  transition: opacity .5s;
  border-radius: 12rpx;
}
.nav-list::after{
  content: '';
  position: absolute;
  bottom: 0px;
  right: 38rpx;
  width: 0;
  height: 0;
  font-size: 0;
  border-top: solid 12rpx transparent;
  border-bottom: solid 12rpx #fff;
  border-left: solid 12rpx transparent;
  border-right: solid 12rpx transparent;
}
.nav-list-open{
  height: auto;
  opacity: 1;
}
.nav-list-item{
  padding:30rpx 40rpx;
  border-bottom: solid 1rpx #666;
}
.nav-icon{
  font-size: 36rpx;
  margin-right: 20rpx;
}

.card-box{
  padding: 20rpx 30rpx;
  background-color: #fff;
  border: solid 1rpx #e6e6e6;
  border-radius: 10rpx;
}
.cell-box{
  padding: 20rpx 30rpx;
  background-color: #fff;
  box-shadow: 0rpx 0rpx 10rpx rgba(0,0,0,.1);
}
.cell-box+.cell-box{
  margin-top: 20rpx;
}
.cell-weui{
  padding: 20rpx 30rpx;
  border-top: solid 1rpx #e6e6e6;
  border-bottom: solid 1rpx #e6e6e6;
  background-color: #fff;
}
.cell-weui+.cell-weui{
  margin-top: 20rpx;
}
.cell-pd{
  padding: 20rpx 30rpx;
}
.line-list{
  display: block;
}
.line-list+.line-list{
  border-top: solid 1rpx #e6e6e6;
  margin-top: 20rpx;
  padding-top: 20rpx;
}
.line-title{
  display: flex;
  align-items:center;
  justify-content: space-between;
  padding-top: 20rpx;
  padding-bottom:20rpx;
  padding-right: 30rpx;
}
.line-title-text{
  padding-left: 22rpx;
  border-left: solid 8rpx #e69422;
  font-size: 36rpx;
}
.line-title-assist{
  font-size: 28rpx;
  color: #999;
  position: relative;
}
.weui-cells .line-title{
  border-top: solid 1rpx #e6e6e6;
  border-bottom: solid 1rpx #e6e6e6;
}
.line-title+.weui-cell:before {
  display: none;
}
.under-title{
  display: flex;
  align-items:center;
  justify-content: space-between;
  padding-top: 20rpx;
  padding-bottom:20rpx;
  position: relative;
}
.under-title-text{
  font-size: 36rpx;
}
.under-title::after{
  content: '';
  position: absolute;
  width: 36rpx;
  height: 8rpx;
  background: #e69422;
  bottom: 0;
  left: 0;
}
.midline-title{
  display: flex;
  align-items:center;
  justify-content: center;
  padding-top: 20rpx;
  padding-bottom:20rpx;
}
.midline-title-text{
  color:#999;
}
.midline-title-text::before, .midline-title-text::after{
  content:'————';
  padding-left: 20rpx;
  padding-right:20rpx;
  color: #e6e6e6;
}
.timeline{
  position: relative;
  padding-left: 60rpx;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
}
.timeline::before{
  content: '';
  position: absolute;
  border-left: dashed 4rpx #e6e6e6;
  height: 100%;
  left: 10rpx;
}
.timeline-text{
  position: relative;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
}
.timeline-text::before{
  content: '';
  position: absolute;
  width: 16rpx;
  height: 16rpx;
  background: #e69422;
  border-radius: 50%;
  left: -54rpx;
  top: 34rpx;
}
.no-msg{
  color:#999;
  text-align: center;
  background: #f2f2f2;
  padding: 20rpx 0;
}
.no-msg::before{
  content:'';
  display: block;
  width: 200rpx;
  height: 200rpx;
  margin: 0 auto;
  background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-position: center center;
  background-size: contain;
}
